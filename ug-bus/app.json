{
  "expo": {
    "name": "Bus Buddy",
    "slug": "Bus Buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.jpg",   // icon.png
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "permissions": [
      "LOCATION",
      "CAMERA"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.jpg", // icon.png
        "backgroundColor": "#ffffff",
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ]
      },
      "package": "com.ullahhafeez321.ugbus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo1.jpg" // favicon.png
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1.jpg", // icon.png
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "431ebac0-b5bf-437f-a85d-7fd1b036a41c"
      }
    }
  }
}
